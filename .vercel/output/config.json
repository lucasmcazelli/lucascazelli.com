{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/concepts/list\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/concepts/([^/]+?)\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/concepts/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "/404.html",
			"status": 404
		}
	],
	"images": {
		"sizes": [
			640,
			750,
			828,
			1080,
			1200,
			1920,
			2048,
			3840
		],
		"domains": [
			"lucascazelli.com"
		],
		"remotePatterns": [
			{
				"protocol": "https",
				"hostname": "**"
			}
		]
	}
}